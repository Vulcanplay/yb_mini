page{ background-color: #f5f5f5;}
.price{ color: red; font-size: 26px;}
.price text{ font-size: 20px;}
.price text.rp{ font-size: 16px; color:#999999;text-decoration: line-through;padding-left:16px;}
.des{ font-size: 16px; color: #666666;}
.city-change{ position: absolute;right: 16px; top: 50%; height: 32px; line-height: 32px; padding: 0 8px;  padding-right: 24px; border-radius: 5px; font-size: 13px; margin-top: -16px; background-color: #f1f1f1; color: #666666;}
.cc-content{position: relative;}
.cc-content image{ position: absolute; right: -18px; top: 9px;}
.title{ border-bottom:1px solid #f5f5f5; background-color: #ffffff; padding:8px 16px; font-size: 15px;}
.title text{ font-size: 13px; color: #666666;}
.has-footer{
  height: 90px;
  width: 100%;
}
.padding-horizontal{
  padding: 16px;
}
.flex-item.title{
  font-size: 14px;
  margin-bottom: 16px;
}
.flex-item{
  font-size: 13px;
  padding: 1px 0;
  color: #555555;
}
.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #ffffff;
  border-top: 1px solid #fafafa;
  min-height: 48px;
  width: 100%;
  text-align: center;
}
.footer .row .border-right{
  border-right: 1px solid #f3f3f3;
  border-radius:0;
}
.footer .row button.col{ background-color: transparent; padding: 0;}
.footer .row .col .one image{
  width: 22px;
  height: 22px;
  padding-top: 4px;
}
.footer .row .col .one{
  height: 28px;
  line-height: 28px;
  text-align: center;
}
.footer .row .col .two{
  height: 20px;
  line-height: 20px;
  text-align: center;
  font-size: 12px;
  color: #999999;
}
.list .item{
  position: relative;
  min-height: 57px;
  padding: 6px 16px;
  margin: 0 auto;
  border-bottom: .55px solid #fafafa;
}
.list .item.ava .item-content{
  margin-left: 70px;
}
.list .item image{
  width: 54px;
  height: 54px;
  position: absolute;
  left: 14px;
  top: 5px;
}
.list .item .item-content .t{
  font-size: 14px;
  color: #555555;
}
.list .item .item-content .note{
  font-size: 13px;
  color: #777888;
}
.list .item .price{
  font-size: 14px;
  font-weight: bold;
  position: absolute;
  right: 12px;
  top: 0;
  line-height: 57px;
  color: #d81e06;
}
.child-list{
  max-height: 208px;
  overflow-y: scroll;
}
swiper{ height:204px;}
.silder-img{
  width: 100%;
  height: 204px;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.button-see-all{
  margin: 0 auto;
  width: 100%;
  text-align: center;
  font-size: 13px;
}

.view-drop-dark{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.7);
  z-index: 3;
}
.view-drop-light{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.3);
  z-index: 1;
}
.city-tip{
  position: fixed;
  top: 30%;
  left: 0;
  width: 100%;
  height: auto;
  background-color: transparent;
  margin: 0 auto;
  z-index: 2;
}
.hb{
  position: fixed;
  top: 30%;
  left: 0;
  width: 100%;
  height: auto;
  z-index: 4;
  text-align: center;
}
.hb image{
  width: 235px;
  height: 286px;
}
.cc-c{
  width: 70%;
  margin: 0 auto;
  padding: 16px 24px;
  background-color: #ffffff;
  border-radius: 4px;
}
.cc-c view{
  font-size: 17px;
  padding-bottom:16px;
  text-align: center;
}
.cc-c .button{
  margin-bottom: 8px;
  border-radius: 32px;
}