/**
 * Grid */
.row {
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -moz-flex;
    display: -ms-flexbox;
    display: flex;
    width: 100%;
}

.row-table {
    text-align: center;
    font-size: 13px;
    color: #9a9a9a;
    background: #fff;
    border-bottom: 1px solid #f2f2f2;
    padding: 10px 0;
}

    .row-table.line-height {
        line-height: 36px;
        padding: 0;
    }

    .row-table .col {
        padding: 5px 0;
    }

.row-wrap {
    -webkit-flex-wrap: wrap;
    -moz-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
}

.row-ell {
    text-overflow: ellipsis;
    overflow: hidden;
}

.row-no-padding {
    padding: 0;
}

    .row-no-padding > .col {
        padding: 0;
    }

.row-select .col {
    margin: 15px 0;
    position: relative;
    font-size: 16px;
}

.row-radio {
    padding: 20px 0px;
}

    .row-radio .col {
        position: relative;
        padding: 5px;
        padding-left: 40px;
    }

    .row-radio .radio-input {
        position: absolute;
        top: 4px;
        left: 20px;
        width: 18px;
        height: 18px;
    }

.row-select .col span {
