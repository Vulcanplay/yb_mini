/**
 * Button *
 */

a.button {
  text-decoration: none; }
.button {
  border-color: transparent;
  background-color: #f8f8f8;
  color: #444;
  position: relative;
  display: inline-block;
  margin: 0;
  padding: 0 12px;
  min-width: 52px;
  min-height: 48px;
  border-width: 0px;
  border-style: solid;
  border-radius: 4px;
  vertical-align: top;
  text-align: center;
  text-overflow: ellipsis;
  font-size: 16px;
  line-height: 48px; }
  
button::after {
  border: 0;
}
  .button:hover {
    color: #444;
    text-decoration: none; }
  .button.active, .button.activated {
    border-color: #a2a2a2;
    background-color: #e5e5e5; }
    
     .button.button-orange {
    border-color: transparent;
    background-color: #ff6d42;
    color: #fff; }
    .button.button-orange:hover {
      background-color: #fd5a2b;
      color: #fff;
      text-decoration: none; }
      
.button-full,
.button-full > .button {
  display: block;
  margin-right: 0;
  margin-left: 0;
  border-right-width: 0;
  border-left-width: 0;
  border-radius: 0; }
  
    .button.button-clear {
      border-color: transparent;
      background: none;
      box-shadow: none;
      color: #666; }

  
.list {
  position: relative;
  padding-top: 1px;
  padding-bottom: 1px;
  padding-left: 0;
  margin-bottom: 20px; }

.list:last-child {
  margin-bottom: 0px; }
  
.item-input {
  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -moz-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  -moz-align-items: center;
  align-items: center;
  position: relative;
  overflow: hidden;
  padding: 6px 0 5px 16px; }
  .item-input input {
    -webkit-border-radius: 0;
    border-radius: 0;
    -webkit-box-flex: 1;
    -webkit-flex: 1 220px;
    -moz-box-flex: 1;
    -moz-flex: 1 220px;
    -ms-flex: 1 220px;
    flex: 1 220px;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    margin: 0;
    padding-right: 24px;
    background-color: transparent; }
    
.item-input input,
.item-input textarea {
  width: 100%; }
  
.item-input input[type="file"],
.item-input input[type="image"],
.item-input input[type="submit"],
.item-input input[type="reset"],
.item-input input[type="button"],
.item-input input[type="radio"],
.item-input input[type="checkbox"] {
  width: auto; }
.modal-box{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  left:0px;
  background:rgba(0,0,0,0.4);
  }

.modal-body{
  position:relative;
  }

.modal-content{
  width: 90%;
  margin: 32px auto;
  padding: 16px 8px 16px 8px;
  background-color: #ffffff;
  overflow: hidden;
  border-radius: 10rpx;}
.modal-content .modal-header .title{
  text-align: center;
  font-size: 16px;
  font-weight: bold;
  border-bottom: 1px solid #e7e7e7;
  padding-bottom: 16px;
}
.modal-content .modal-header .button {
  position: absolute;
  right: 12px;
  top: 12px;
}
.modal-content .modal-header .button image{
  width: 25px;
  height:25px;
}